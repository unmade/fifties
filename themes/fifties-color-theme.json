        "peekViewResult.fileForeground": "#eae8e1",
        "peekViewResult.lineForeground": "#c5c0af",
        "peekViewResult.selectionForeground": "#eae8e1",
        "peekViewTitle.background": "#262521",
        "peekViewTitleDescription.foreground": "#757166",
        "peekViewTitleLabel.foreground": "#c5c0af",
    },
    "tokenColors": [

        // General

        {
            "name": "Builtins",
            "scope": [
                "support.function.builtin",
            ],
            "settings": {
                "foreground": "#90bb8f",
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#757166"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "keyword.other.unit",
            ],
            "settings": {
                "foreground": "#e992a8"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function",
            ],
            "settings": {
                "foreground": "#FACA78",
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type",
                "keyword.operator.new",
                "punctuation.section.embedded.begin",
                "punctuation.section.embedded.end",
            ],
            "settings": {
                "foreground": "#db7c45",
                "fontStyle": "",
            }
        },
        {
            "name": "No color",
            "scope": [
                "entity.other.inherited-class",
                "meta.embedded.expression",
                "storage.modifier",
                "keyword.operator",
            ],
            "settings": {
                "foreground": "#c5c0af",
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric",
            ],
            "settings": {
                "foreground": "#7fb3bd"
            }
        },
        {
            "name": "Self",
            "scope": [
                "variable.parameter.function.language.special.self",
                "variable.language.special.self",
                "variable.language.special.cls",
                "variable.language.this",
            ],
            "settings": {
                "foreground": "#aa8c4a",
            }
        },
        {
            "name": "String, Symbols, Markup Heading",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key",
                "markup.heading",
                "markup.inserted.git_gutter",
            ],
            "settings": {
                "foreground": "#739B89"
            }
        },
        {
            "name": "Super",
            "scope": [
                "variable.language.super",
            ],
            "settings": {
                "foreground": "#90bb8f",
            }
        },
        {
            "name": "Support type",
            "scope": [
                "support.type",
            ],
            "settings": {
                "foreground": "#a0613d",
                "fontStyle": "italic"
            }
        },
        {
            "name": "React Tag",
            "scope": [
                "entity.name.tag support.class",
            ],
            "settings": {
                "foreground": "#90bb8f",
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter",
                "punctuation.definition.tag",
            ],
            "settings": {
                "foreground": "#a0613d"
            }
        },
        {
            "name": "Tag attributes",
            "scope": [
                "meta.tag.attributes",
                "entity.other.attribute-name",
            ],
            "settings": {
                "foreground": "#aa8c4a",
            }
        },

        // C

        {
            "name": "C: keywords",
            "scope": [
                "source.c storage.modifier",
            ],
            "settings": {
                "foreground": "#db7c45",
                "fontStyle": "",
            }
        },
        {
            "name": "C: preprocessor",
            "scope": [
                "source.c entity.name.function.preprocessor",
                "source.cpp entity.name.function.preprocessor",
            ],
            "settings": {
                "foreground": "#90bb8f",
            }
        },
        {
            "name": "C: built-in type",
            "scope": [
                "source.c storage.type.built-in",
                "source.cpp storage.type.built-in",
                "source.c storage.type.built-in.primitive",
                "source.cpp storage.type.built-in.primitive",
            ],
            "settings": {
                "foreground": "#a0613d",
                "fontStyle": "italic"
            }
        },

        // CSS

        {
            "name": "CSS: keywords",
            "scope": [
                "source.css keyword.operator.logical",
            ],
            "settings": {
                "foreground": "#db7c45",
            }
        },
        {
            "name": "CSS: tag name",
            "scope": [
                "source.css entity.name.tag",
            ],
            "settings": {
                "foreground": "#90bb8f",
            }
        },
        {
            "name": "CSS: attribute name",
            "scope": [
                "source.css entity.other.attribute-name.id",
                "source.css entity.other.attribute-name.class",
            ],
            "settings": {
                "foreground": "#90bb8f",
            }
        },
        {
            "name": "CSS: property keys",
            "scope": [
                "source.css support.type.property-name",
                "source.css meta.property-name support.type",
            ],
            "settings": {
                "foreground": "#a0613d",
                "fontStyle": "",
            }
        },
        {
            "name": "CSS: property values",
            "scope": [
                "source.css keyword.other.unit",
                "source.css meta.property-value support.constant",
                "source.css support.constant.media",
            ],
            "settings": {
                "foreground": "#c5c0af",
            }
        },
        {
            "name": "CSS: functions",
            "scope": [
                "source.css support.function",
            ],
            "settings": {
                "foreground": "#FACA78",
            }
        },
        {
            "name": "CSS: pseudo-class",
            "scope": [
                "source.css entity.other.attribute-name.pseudo-class",
            ],
            "settings": {
                "foreground": "#aa8c4a",
            }
        },
        {
            "name": "CSS: hex colors",
            "scope": [
                "source.css constant.other.color.rgb-value.hex",
            ],
            "settings": {
                "foreground": "#7fb3bd",
            }
        },

        // Go

        {
            "name": "Go: Function name",
            "scope": [
                "source.go support.function",
            ],
            "settings": {
                "foreground": "#FACA78",
            }
        },
        {
            "name": "Go: built-in type",
            "scope": [
                "source.go storage.type",
            ],
            "settings": {
                "foreground": "#a0613d",
            }
        },

        // ini

        {
            "name": "ini: section",
            "scope": [
                "source.ini entity.name.section.group-title"
            ],
            "settings": {
                "foreground": "#90bb8f",
            }
        },
        {
            "name": "ini: attribute name",
            "scope": [
                "source.ini keyword.other",
            ],
            "settings": {
                "foreground": "#a0613d",
            }
        },

        // Javascript

        {
            "name": "Javascript: 'constructor'",
            "scope": [
                "source.js meta.method.declaration storage.type",
            ],
            "settings": {
                "foreground": "#FACA78",
                "fontStyle": ""
            }
        },
        {
            "name": "Javascript: keywords and braces",
            "scope": [
                "source.js storage.modifier",
                "source.js meta.method.declaration meta.var.expr storage.type.js",
                "source.js meta.object-literal.key meta.brace",
                "source.js punctuation.definition.template-expression.begin",
                "source.js punctuation.definition.template-expression.end",
                "source.js keyword.operator.expression.of",
                "source.js keyword.operator.expression.in",
                "source.js keyword.operator.expression.instanceof"
            ],
            "settings": {
                "foreground": "#db7c45",
                "fontStyle": ""
            }
        },
        {
            "name": "Javascript: no color",
            "scope": [
                "source.js storage.type.function.arrow",
                "source.js punctuation.separator.key-value",
                "source.js constant.language.import-export-all",
                "source.js meta.object-literal.key variable",
                "source.js meta.object-literal.key punctuation",
                "source.js meta.template.expression variable",
                "source.js meta.template.expression punctuation",
                "source.js meta.template.expression meta",
            ],
            "settings": {
                "foreground": "#c5c0af",
            }
        },
        {
            "name": "Javascript: object literal keys",
            "scope": [
                "source.js meta.object-literal.key",
                "source.js meta.object-literal.key string",
                "source.js meta.object-literal.key punctuation.definition.string",
            ],
            "settings": {
                "foreground": "#a0613d",
            }
        },
        {
            "name": "Javascript: string quotes inside string interpolation",
            "scope": [
                "source.js meta.template.expression string punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#739B89",
            }
        },

        // JSON

        {
            "name": "JSON",
            "scope": [
                "source.json support.type.property-name",
            ],
            "settings": {
                "fontStyle": "",
            }
        },

        // Markdown

        {
            "name": "Markdown: bold",
            "scope": [
                "text.html.markdown markup.bold",
            ],
            "settings": {
                "fontStyle": "bold",
            }
        },
        {
            "name": "Markdown: bold italic",
            "scope": [
                "text.html.markdown markup.italic markup.bold",
                "text.html.markdown markup.bold markup.italic",
            ],
            "settings": {
                "fontStyle": "bold italic",
            }
        },
        {
            "name": "Markdown: code block",
            "scope": [
                "text.html.markdown markup.fenced_code punctuation.definition.markdown",
                "text.html.markdown fenced_code.block.language.markdown"
            ],
            "settings": {
                "foreground": "#757166",
            }
        },
        {
            "name": "Markdown: heading",
            "scope": [
                "text.html.markdown markup.heading",
            ],
            "settings": {
                "foreground": "#a0613d"
            }
        },
        {
            "name": "Markdown: italic",
            "scope": [
                "text.html.markdown markup.italic",
            ],
            "settings": {
                "fontStyle": "italic",
            }
        },
        {
            "name": "Markdown: links",
            "scope": [
                "text.html.markdown meta.link"
            ],
            "settings": {
                "foreground": "#7fb3bd",
            }
        },
        {
            "name": "Markdown: link title",
            "scope": [
                "text.html.markdown meta.link string.other.link.title",
                "text.html.markdown meta.link punctuation.definition.string.begin",
                "text.html.markdown meta.link punctuation.definition.string.end"
            ],
            "settings": {
                "foreground": "#FACA78",
            }
        },
        {
            "name": "Markdown: quote",
            "scope": [
                "text.html.markdown markup.quote",
            ],
            "settings": {
                "foreground": "#757166"
            }
        },

        // Python

        {
            "name": "Python: and/in/or, True, False, None",
            "scope": [
                "source.python constant.language.python",
                "source.python keyword.operator.logical",
                "source.python constant.character.format.placeholder.other"
            ],
            "settings": {
                "foreground": "#db7c45",
                "fontStyle": ""
            }
        },
        {
            "name": "Python: built-in type annotation",
            "scope": [
                "source.python support.type",
                "source.python meta.item-access.arguments support.type",
                "source.python meta.function.decorator meta.function-call.arguments support.type",
            ],
            "settings": {
                "foreground": "#a0613d",
                "fontStyle": ""
            }
        },
        {
            "name": "Python: built-in type call",
            "scope": [
                "source.python support.function.builtin",
                "source.python meta.function-call.python support.type",
                "source.python meta.item-access meta.function-call.python support.type",
            ],
            "settings": {
                "foreground": "#90bb8f",
                "fontStyle": ""
            }
        },
        {
            "name": "Python: decorators",
            "scope": [
                "source.python meta.function.decorator support.type", // Handle textmate bug involving class decorator args
                "source.python meta.function.decorator entity.name.function.decorator",
            ],
            "settings": {
                "foreground": "#73A1B0",
                "fontStyle": "",
            }
        },
        {
            "name": "Python: docstring",
            "scope": [
                "source.python string.quoted.docstring",
                "source.python string.quoted.docstring keyword.control.flow"
            ],
            "settings": {
                "foreground": "#739B89",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Python: exceptions",
            "scope": [
                "source.python support.type.exception"
            ],
            "settings": {
                "foreground": "#90bb8f",
                "fontStyle": ""
            }
        },
        {
            "name": "Python: global, nonlocal",
            "scope": [
                "source.python storage.modifier.declaration"
            ],
            "settings": {
                "foreground": "#db7c45",
                "fontStyle": ""
            }
        },
        {
            "name": "Python: magic vars, function and methods",
            "scope": [
                "source.python support.function.magic",
                "source.python support.variable.magic"
            ],
            "settings": {
                "foreground": "#e992a8"
            }
        },
        {
            "name": "Python: named arguments",
            "scope": [
                "source.python variable.parameter.function-call"
            ],
            "settings": {
                "foreground": "#b14419",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Python: strings",
            "scope": [
                "source.python string",
                "source.python meta.fstring.python storage.type",
                "source.python meta.fstring.python constant.character.escape.python"
            ],
            "settings": {
                "foreground": "#739B89"
            }
        },

        // TOML

        {
            "name": "TOML: section",
            "scope": [
                "source.toml entity.other.attribute-name",
            ],
            "settings": {
                "foreground": "#c5c0af",
            }
        },
        {
            "name": "TOML: attribute name",
            "scope": [
                "source.toml keyword.key",
            ],
            "settings": {
